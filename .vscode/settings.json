{
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "git.branchPrefix": "PAY/",
    "git.branchProtection": [
        "master",
        "staging"
    ],
    "git.branchValidationRegex": "^(PAY-[a-z0-9._-]+)$",
    "git.autofetch": true,
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "cSpell.words": [
        "mrsp"
    ]
}
